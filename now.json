{
  "version": 2,
  "name": "now-node-express-basic-auth-example",
  "alias": [
    "now-express-basic-auth-example"
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "builds": [
    {
      "src": "/index.js",
      "use": "@now/node",
      "config": {
        "includeFiles": [
          "public/**"
        ]
      }
    }
  ]
}
